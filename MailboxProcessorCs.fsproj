<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>MailboxProcessorCs</AssemblyName>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <OutputType>Library</OutputType>
    <Version>1.0.0</Version>
		<FileVersion>1.0.0.0</FileVersion>
		<AssemblyVersion>1.0.0.0</AssemblyVersion>
		<Authors>Uwe Riegel</Authors>
    <Company>Uwe Riegel</Company>		
		<Description>MailboxProcessor for use in CSharp</Description>
    <PackageLicenseUrl></PackageLicenseUrl>
		<PackageProjectUrl>https://github.com/uriegel/MailboxProcessorCs</PackageProjectUrl>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageTags>FSharp</PackageTags>
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>      
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Agent.fs" />
  </ItemGroup>

  <ItemGroup>
    	<None Include=".\LICENSE">
      		<Pack>True</Pack>
      	<PackagePath></PackagePath>
    	</None>
  </ItemGroup>  

</Project>
